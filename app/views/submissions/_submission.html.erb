<li id="submission-<%= submission.id %>">
  <%# <%= link_to gravatar_for(micropost.user, size: 50), micropost.user %>
  <%= link_to show_icon(submission.atcoder_user), "https://atcoder.jp/users/#{submission.atcoder_user.atcoder_id}"%>
  <%# link_to "#",  %>
  <span class="contest_title">
    <%= link_to submission.atcoder_user.atcoder_id,
    "https://atcoder.jp/users/#{submission.atcoder_user.atcoder_id}" %>
  </span> 
  <span class="content">
    <%= link_to submission.contest.title,
    "https://atcoder.jp/contests/#{submission.contest.name}" %>
    <%= link_to submission.problem.title,
    "https://atcoder.jp/contests/#{submission.contest.name}/tasks/#{submission.problem.name}" %>
    <%= submission.result %>
  </span>

  <span class="content"> </span>
  <span class="content">
  <%# <%= show_result(@history,contest) %> 
  </span>
  <span class="timestamp">
    <%= submission.language %>
    <%= Time.at(submission.epoch_second) %>
    <%# <%= @atcoder_user.atcoder_id %>
    <%# Posted <%= time_ago_in_words(contest.created_at) %>
  </span>
</li>